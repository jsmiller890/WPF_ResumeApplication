<Window x:Class="WPF_ResumeApplication.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WPF_ResumeApplication"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <Button Content="Song List" HorizontalAlignment="Left" Margin="32,30,0,0" VerticalAlignment="Top" Width="129" Height="86" Command="{Binding OpenNewViewCommand}" CommandParameter="SongList"/>
        <Button Content="Calculator" HorizontalAlignment="Left" Margin="687,335,0,0" VerticalAlignment="Top" Width="79" Height="62" Command="{Binding OpenNewViewCommand}" CommandParameter="Calculator"/>
    </Grid>
</Window>
